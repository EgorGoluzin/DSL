digraph LOOP {
  rankdir=LR;
  node [fontname="Arial"];
  start [label="Loop" shape=plaintext];
  end [label="" shape=point];
  A [label="LS" shape=box];
  B [label="LB" shape=box];
  C [label="LA" shape=box];
  D [label="LC" shape=box];
  start -> D;
  start -> C;
  start -> B;
  start -> A;
  A -> end;
  B -> end;
  C -> end;
  D -> end;
}