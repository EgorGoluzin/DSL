digraph Rule {
    start [label = "Rule" shape = plaintext];
    end [label = "" shape = point];
    A [label = "name" shape = diamond];
    B [label = "::=" shape = oval];
    C [label = "Alternative" shape = box];
    D [label="." shape=oval];
    E [label=";" shape=oval];
    start -> A;
    A -> B;
    B -> C;
    C -> D;
    C -> E;
    D -> end;
    E -> end;
}