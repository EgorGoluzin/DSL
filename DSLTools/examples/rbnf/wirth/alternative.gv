diagraph Alternative {
    start [label = "Alternative" shape = plaintext];
    end [label = "" shape = point];
    A [label = "RuleElement" shape = box];
    B [label = "{" shape = oval];
    C [label = "|" shape = oval];
    D [label = "#" shape = oval];
    E [label = "RuleElement" shape = box];
    F [label = "}" shape = oval];
    start -> A;
    A -> B;
    B -> C;
    C -> D;
    D -> E;
    E -> F;
    F -> end;
}