digraph RANGE {
  rankdir=LR;
  node [fontname="Arial"];
  start [label="Range" shape=plaintext];
  end [label="" shape=point];
  A [label="FROM" shape=oval];
  B [label="Number" shape=box];
  C [label="TO" shape=oval];
  D [label="Number" shape=box];
  start -> A;
  A -> B;
  B -> C;
  C -> D;
  D -> end;
}