digraph LC {
  rankdir=LR;
  node [fontname="Arial"];
  start [label="LC" shape=plaintext];
  end [label="" shape=point];
  A [label="FOR" shape=oval];
  B [label="Variable" shape=box];
  C [label="Range" shape=box];
  D [label="DO" shape=oval];
  E [label="Block" shape=box];
  F [label="END_FOR" shape=oval];
  start -> A;
  A -> B;
  B -> C;
  C -> D;
  D -> E;
  E -> F;
  F -> end;
}