digraph ERROR_BLOCK {
    start [label=ERROR_BLOCK shape=plaintext]
    A [label=ERRORS shape=oval]
    B [label=":" shape=oval]
    C [label=NONTERMINAL shape=box]
    D [label=string shape=box]
    E [label="|" shape=oval]
    F [label=";" shape=oval]
    G [label="." shape=oval]
